{
  "manifest_version": 2,
  "version": "1",
  "name": "TeamTodoList - Mortal Kombat theme",
  "web_accessible_resources": ["background.png"],
  "content_scripts":[
    {
      "matches": ["http://teamtodolist.com/teams/*"],
      "css":["main.css"]
    },
    {
      "matches": ["http://teamtodolist.com/teams/*/todos/*"],
      "css":["todos.css"]
    }
  ]
}
